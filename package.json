{
  "private": true,
  "name": "history-hub",
  "description": "Browse and explore your Claude Code conversation history",
  "version": "1.0.0",
  "author": {
    "name": "Nate",
    "email": "nate@example.com"
  },
  "main": "app/background.js",
  "scripts": {
    "dev": "nextron",
    "build": "yarn build:native && nextron build",
    "build:mac": "yarn build:native && nextron build --mac",
    "build:win": "yarn build:native && nextron build --win",
    "build:linux": "yarn build:native && nextron build --linux",
    "build:all": "yarn build:native && nextron build --mac --win --linux",
    "build:native": "cd native && . $HOME/.cargo/env && npm run build",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "claude-parser": "file:./native",
    "electron-serve": "^1.3.0",
    "electron-store": "^8.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.52",
    "autoprefixer": "^10.4.19",
    "electron": "^34.0.0",
    "electron-builder": "^24.13.3",
    "next": "^14.2.4",
    "nextron": "^9.5.0",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.7.3"
  }
}
